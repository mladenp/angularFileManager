{
  "name": "angularfilemanager",
  "private": true,
  "version": "0.0.0",
  "description": "A prototype for file manager in angular",
  "repository": "https://github.com/mladenp/angularFileManager",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.3.1",
    "gulp": "^3.9.1",
    "gulp-livereload": "^3.8.1",
    "http-server": "^0.6.1",
    "jasmine-core": "^2.3.4",
    "karma": "~0.12",
    "karma-chrome-launcher": "^0.1.12",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.5",
    "karma-junit-reporter": "^0.2.2",
    "live-reload": "^1.1.0",
    "protractor": "^2.1.0",
    "shelljs": "^0.2.6"
  },
  "scripts": {
    "replace-name-preview": "/node_modules/replace/bin/replace.js myApp YOUR_APP_NAME app -r -p --include='*.js,*.html' --exclude=bower_components",
    "replace-name": "/node_modules/replace/bin/replace.js myApp YOUR_APP_NAME app -r --include='*.js,*.html' --exclude=bower_components",

    "postinstall": "bower install",
    "prestart": "npm install",
    "start": "http-server ./app -a localhost -p 8000 -c-1",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js  --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor protractor.conf.js"
  }
}
